{
  "name": "cashback-help-bot",
  "version": "1.0.0",
  "description": "Telegram bot for searching MCC codes by cashback categories for various banks",
  "main": "src/app.ts",
  "scripts": {
    "dev": "tsx --env-file=.env --watch src/app.ts",
    "build": "tsc",
    "start": "tsx --env-file=.env src/app.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate",
    "migration:run": "typeorm-ts-node-commonjs migration:run",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert",
    "seed": "tsx src/database/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "get-telegram-id": "tsx --env-file=.env scripts/get-telegram-id.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "cashback",
    "mcc",
    "banking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@sentry/node": "^8.0.0",
    "@telegraf/session": "^2.0.0-beta.7",
    "dotenv": "^16.4.0",
    "kysely": "^0.27.2",
    "langchain": "^0.3.0",
    "lru-cache": "^11.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.0",
    "prom-client": "^15.1.0",
    "reflect-metadata": "^0.2.0",
    "telegraf": "^4.16.0",
    "typeorm": "^0.3.20",
    "winston": "^3.14.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
